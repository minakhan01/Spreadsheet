import javax.swing.JPanel;


public class RowView extends JPanel {
	
	Cell[] row;
	int numColumns;
	RowView row_view;
	public RowView(int numColumns){
		this.numColumns=numColumns;
		row=new Cell[numColumns];
	}
	
	public int getTotal(){
		int sum=0;
		for (int i=0; i<numColumns;i++){
			sum += row[i].value;
		}
		return sum;
	}
	
}
